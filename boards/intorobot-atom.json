{
    "build": {
        "core": "intorobot",
        "cpu": "cortex-m3",
        "extra_flags": "-DSTM32F103xB -DPLATFORM_ID=888004 -DPLATFORM_NAME=atom",
        "f_cpu": "72000000L",
        "hwids": [
            ["0x0483", "0xdf11"],
            ["0x0483", "0x5740"]
        ],
        "ldscript": "linker_stm32f103xb_app.ld",
        "startup_object": "startup_stm32f103xb.S.o",
        "mcu": "STM32F1",
        "variant": "intorobot-atom"
    },
    "frameworks": [
        "intorobot",
        "mbed",
        "arduino"
    ],
    "name": "IntoRobot atom(128k flash, 20k sram)",
    "upload": {
        "disable_flushing": false,
        "maximum_ram_size": 20480,
        "maximum_size": 102400,
        "protocol": "dfu",
        "require_upload_port": true,
        "use_1200bps_touch": false,
        "wait_for_upload_port": false,
        "address": "0x08007000"
    },
    "url": "https://www.intoyun.com/hardware",
    "vendor": "IntoRobot",
    "debug": {
        "tools": {
            "stlink-v2": {
                "server": {
                    "package": "tool-openocd",
                    "executable": "bin/openocd",
                    "arguments": [
                        "-f", "scripts/interface/stlink-v2.cfg",
                        "-c", "transport select hla_swd",
                        "-f", "scripts/target/stm32f4x.cfg"
                    ]
                },
                "default": true
            },
            "stlink-v2-1": {
                "server": {
                    "package": "tool-openocd",
                    "executable": "bin/openocd",
                    "arguments": [
                        "-f", "scripts/interface/stlink-v2-1.cfg",
                        "-c", "transport select hla_swd",
                        "-f", "scripts/target/stm32f4x.cfg"
                    ]
                }
            }
        }
    }
}

